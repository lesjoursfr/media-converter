{
	"name": "@lesjoursfr/media-converter",
	"version": "3.0.2",
	"description": "CLI for the fluent-ffmpeg npm module.",
	"keywords": [
		"cli",
		"ffmpeg",
		"converter",
		"fluent-ffmpeg"
	],
	"author": "",
	"license": "MIT",
	"repository": "lesjoursfr/media-converter",
	"homepage": "https://github.com/lesjoursfr/media-converter#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/media-converter/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": "22.x || 24.x"
	},
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm yarn.lock && yarn",
		"ljconverter": "NODE_ENV=development ts-node src/index.ts",
		"check-lint": "eslint",
		"check-format": "prettier --check .",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint --fix",
		"format": "prettier --write .",
		"build": "tsc"
	},
	"bin": {
		"ljconverter": "bin/index.js"
	},
	"dependencies": {
		"fluent-ffmpeg": "^2.1.3",
		"picocolors": "^1.1.1",
		"progress": "^2.0.3",
		"yargs": "^18.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@tsconfig/node22": "^22.0.5",
		"@types/fluent-ffmpeg": "^2.1.28",
		"@types/node": "^24.10.4",
		"@types/progress": "^2.0.7",
		"@types/yargs": "^17.0.35",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"globals": "^17.0.0",
		"prettier": "^3.7.4",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.52.0"
	},
	"packageManager": "yarn@4.12.0"
}
