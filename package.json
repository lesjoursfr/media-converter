{
	"name": "@lesjoursfr/media-converter",
	"version": "1.0.0",
	"description": "CLI for the fluent-ffmpeg npm module.",
	"keywords": [
		"cli",
		"ffmpeg",
		"converter",
		"fluent-ffmpeg"
	],
	"author": "",
	"license": "MIT",
	"repository": "lesjoursfr/media-converter",
	"homepage": "https://github.com/lesjoursfr/media-converter#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/media-converter/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=v16.15.0"
	},
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm -r .yarn/cache/ && rm yarn.lock && yarn",
		"ljconverter": "NODE_ENV=development ts-node src/index.ts",
		"lint": "npx eslint --ext .ts --fix src/",
		"build": "npx tsc"
	},
	"bin": {
		"ljconverter": "bin/index.js"
	},
	"dependencies": {
		"fluent-ffmpeg": "^2.1.2",
		"picocolors": "^1.0.0",
		"progress": "^2.0.3",
		"yargs": "^17.4.1"
	},
	"devDependencies": {
		"@tsconfig/node16": "^1.0.2",
		"@types/fluent-ffmpeg": "^2.1.20",
		"@types/node": "^16.11.33",
		"@types/progress": "^2.0.5",
		"@types/yargs": "^17.0.10",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.4"
	},
	"packageManager": "yarn@3.2.0"
}
